
<div class="fos_user_user_show">
    <p>Prenom: {{ user.prenom }}</p>
    <p>Nom: {{ user.nom }}</p>
    <p>Nom d'utilisateur: {{ user.username }}</p>
    <p>Email: {{ user.email }}</p>
    <p>Organisme: {{ user.organisme }}</p>
    <p>Fonction: {{ user.poste }}</p>
    <p>Téléphone: {{ user.telephone }}</p>
    {#<img src="{{ asset('images/profile_pictures/{{ user.pictureUrl }}' }}"/>#}
    {% if (app.user.picture != null) %}
        <img width="50" src="{{ path('user_get_picture', {'picture': app.user.picture}) }}" />
    {% endif %}
    <a href="{{ path('fos_user_profile_edit') }}">Modifier mon profil</a>
</div>
